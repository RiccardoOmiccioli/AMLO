#################################################################
#    Classes
#################################################################

###  http://www.a2rd.net.br/mlo#Institutions
mlo:Institutions rdf:type owl:Class ;
        rdfs:subClassOf foaf:Organization ;
        rdfs:isDefinedBy mlo: ;
        rdfs:label "Company"@en ,
                "Azienda"@it .

###  http://xmlns.com/foaf/0.1/Agent
foaf:Agent rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Class ;
                owl:unionOf ( foaf:Group
                        foaf:Person
                        foaf:Organization
                )
        ] ;
        rdfs:comment "An agent (eg. person, group, software or physical artifact)."@en ;
        rdfs:isDefinedBy foaf: ;
        rdfs:label "Agent"@en ,
                "Agente"@it .

###  http://xmlns.com/foaf/0.1/Group
foaf:Group rdf:type owl:Class ;
        rdfs:subClassOf foaf:Agent ;
        rdfs:comment "A collection of individual agents."@en ;
        rdfs:isDefinedBy foaf: ;
        rdfs:label "Group"@en ,
                "Gruppo"@it .

###  http://xmlns.com/foaf/0.1/Organization
foaf:Organization rdf:type owl:Class ;
        rdfs:subClassOf foaf:Agent ;
        owl:equivalentClass [ rdf:type owl:Class ;
                owl:unionOf ( mlo:Institutions
                        aiiso:Institute
                        aiiso:College
                )
        ] ;
        rdfs:comment "An organization."@en ;
        rdfs:isDefinedBy foaf: ;
        rdfs:label "Organization"@en ,
                "Ente"@it .

### http://purl.org/vocab/aiiso/schema#Institute
aiiso:Institute rdf:type owl:Class ;
        rdfs:subClassOf foaf:Organization ;
        owl:disjointWith mlo:Institutions,
                aiiso:College ;
        rdfs:comment "An Institute is a group of people recognised by an organization as forming a cohesive group referred to by the organization as an institute."@en ;
        rdfs:isDefinedBy aiiso: ;
        rdfs:label "Institute"@en ,
                "Istituto"@it .

### http://purl.org/vocab/aiiso/schema#College
aiiso:College rdf:type owl:Class ;
        rdfs:subClassOf foaf:Organization ;
        owl:disjointWith mlo:Institutions,
                aiiso:Institute;
        rdfs:comment "A College is a group of people recognised by an organization as forming a cohesive group referred to by the organization as a college."@en ;
        rdfs:isDefinedBy aiiso: ;
        rdfs:label "College"@en ,
                "Università"@it .

###  http://xmlns.com/foaf/0.1/Person
foaf:Person rdf:type owl:Class ;
        rdfs:subClassOf foaf:Agent ;
        rdfs:comment "A person."@en ;
        rdfs:isDefinedBy foaf: ;
        rdfs:label "Person"@en ,
                "Persona"@it .

###  https://github.com/RiccardoOmiccioli/AMLO/Role
:Role rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Class ;
                owl:unionOf ( :Author
                        :Organizer
                )
        ] ;
        rdfs:label "Role"@en ,
                "Ruolo"@it .

###  https://github.com/RiccardoOmiccioli/AMLO/Author
:Author rdf:type owl:Class ;
        rdfs:subClassOf :Role ;
        rdfs:comment "An author of a project."@en ;
        rdfs:label "Author"@en ,
                "Autore"@it .

###  https://github.com/RiccardoOmiccioli/AMLO/Organizer
:Organizer rdf:type owl:Class ;
        rdfs:subClassOf :Role ;
        rdfs:comment "An organizer of an event."@en ;
        rdfs:label "Organizer"@en ,
                "Organizzatore"@it .

###  http://xmlns.com/foaf/0.1/Project
foaf:Project rdf:type owl:Class ;
        rdfs:comment "A project (a collective endeavour of some kind)."@en ;
        rdfs:isDefinedBy foaf: ;
        rdfs:label "Project"@en ,
                "Progetto"@it .

###  http://dbpedia.org/ontology/Competition
dbo:Competition rdf:type owl:Class ;
        rdfs:isDefinedBy dbo: ;
        rdfs:label "Competition"@en ,
                "Competizione"@it .

###  http://dbpedia.org/ontology/Year
dbo:Year rdf:type owl:Class ;
        rdfs:isDefinedBy dbo: ;
        rdfs:label "Anno"@it ,
                "Year"@en .

###  https://github.com/RiccardoOmiccioli/AMLO/Edition
:Edition rdf:type owl:Class ;
        rdfs:label "Edition"@en ,
                "Edizione"@it .

###  http://www.a2rd.net.br/mlo#Algorithms
mlo:Algorithms rdf:type owl:Class ;
        rdfs:comment "A concrete implementation of a theoretical concept of the AI field."@en ;
        rdfs:isDefinedBy mlo:;
        rdfs:label "Technology"@en ,
                "Tecnologia"@it .

###  https://github.com/RiccardoOmiccioli/AMLO/NeuralNetwork
:NeuralNetwork rdf:type owl:Class ;
        rdfs:subClassOf mlo:Algorithms ;
        owl:equivalentClass [ rdf:type owl:Class ;
                owl:unionOf ( :ConvolutionalNeuralNetwork
                        :RecurrentNeuralNetwork
                )
        ] ;
        rdfs:comment "todo"@en ;
        rdfs:label "Neural Network"@en ,
                "Rete Neurale"@it .

###  https://github.com/RiccardoOmiccioli/AMLO/ConvolutionalNeuralNetwork
:ConvolutionalNeuralNetwork rdf:type owl:Class ;
        rdfs:subClassOf :NeuralNetwork ;
        rdfs:comment "todo"@en ;
        rdfs:label "Convolutional Neural Network"@en ,
                "Rete Neurale Convoluzionale"@it .

###  https://github.com/RiccardoOmiccioli/AMLO/RecurrentNeuralNetwork
:RecurrentNeuralNetwork rdf:type owl:Class ;
        rdfs:subClassOf :NeuralNetwork ;
        rdfs:comment "todo"@en ;
        rdfs:label "Recurrent Neural Network"@en ,
                "Rete Neurale Ricorrente"@it .

###  https://github.com/RiccardoOmiccioli/AMLO/Tool
:Tool rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Class ;
                owl:unionOf ( :ActivationFunction
                        :GradientDescent
                        :Layer
                )
        ] ;
        rdfs:comment "A theoretical concept of the mathematical or AI field."@en ;
        rdfs:label "Tool"@en ,
                "Strumento"@it .

###  https://github.com/RiccardoOmiccioli/AMLO/Layer
:Layer rdf:type owl:Class ;
        rdfs:subClassOf :Tool ;
        owl:equivalentClass [ rdf:type owl:Restriction ;
                owl:onProperty :hasLayerType ;
                owl:someValuesFrom :LayerType
        ] ;
        rdfs:comment "A single layer of neurons of a Neural Network"@en ;
        rdfs:label "Layer"@en ,
                "Strato"@it .

###  https://github.com/RiccardoOmiccioli/AMLO/GradientDescent
:GradientDescent rdf:type owl:Class ;
        rdfs:subClassOf :Tool ;
        rdfs:comment "todo"@en ;
        rdfs:label "Gradient Descent"@en ,
                "Discesa del Gradiente"@it .

###  https://github.com/RiccardoOmiccioli/AMLO/ActivationFunction
:ActivationFunction rdf:type owl:Class ;
        rdfs:subClassOf :Tool ;
        rdfs:comment "todo"@en ;
        rdfs:label "Activation Function"@en ,
                "Funzione di Attivazione"@it .

###  https://github.com/RiccardoOmiccioli/AMLO/Category
:Category rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Class ;
                owl:unionOf ( :MachineLearningType
                        :LayerType
                )
        ] ;
        rdfs:label "Category"@en ,
                "Categoria"@it .

###  https://github.com/RiccardoOmiccioli/AMLO/MachineLearningType
:MachineLearningType rdf:type owl:Class ;
        rdfs:subClassOf :Category ;
        owl:equivalentClass [ rdf:type owl:Class ;
                owl:unionOf ( :ReinforcementLearning
                        :SupervisedLearning
                        :UnsupervisedLearning
                )
        ] ;
        rdfs:label "Machine Learning Type"@en ,
                "Tipo Machine Learning"@it .

###  https://github.com/RiccardoOmiccioli/AMLO/ReinforcementLearning
:ReinforcementLearning rdf:type owl:Class ;
        rdfs:subClassOf :MachineLearningType ;
        rdfs:label "Reinforcement Learning"@en ,
                "Apprendimento per Rinforzo"@it .

###  https://github.com/RiccardoOmiccioli/AMLO/SupervisedLearning
:SupervisedLearning rdf:type owl:Class ;
        rdfs:subClassOf :MachineLearningType ;
        owl:equivalentClass [ rdf:type owl:Class ;
                owl:oneOf ( :Classification
                        :Regression
                )
        ] ;
        rdfs:label "Supervised Learning"@en ,
                "Apprendimento Supervisionato"@it .

###  https://github.com/RiccardoOmiccioli/AMLO/UnsupervisedLearning
:UnsupervisedLearning rdf:type owl:Class ;
        rdfs:subClassOf :MachineLearningType ;
        owl:equivalentClass [ rdf:type owl:Class ;
                owl:oneOf ( :Clustering
                        :Dimensionality_Reduction
                )
        ] ;
        rdfs:label "Unsupervised Learning"@en ,
                "Apprendimento non Supervisionato"@it .

###  https://github.com/RiccardoOmiccioli/AMLO/LayerType
:LayerType rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Class ;
                owl:oneOf ( :Convolution
                        :Fully_Connected
                        :Pooling
                )
        ] ;
        rdfs:subClassOf :Category ;
        rdfs:label "Layer Type"@en ,
                "Tipo Strato"@it .

