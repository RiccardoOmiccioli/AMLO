        rdf:type owl:AsymmetricProperty ,
                owl:IrreflexiveProperty ;
        rdfs:domain :Edition ;
        rdfs:range dbo:Year ;
        rdfs:label "has edition year"@en .

###  https://github.com/RiccardoOmiccioli/AMLO/isEditionYearOf
:isEditionYearOf rdf:type owl:ObjectProperty ;
        rdfs:label "is edition year of"@en .

###  https://github.com/RiccardoOmiccioli/AMLO/isOrganizedBy
:isOrganizedBy rdf:type owl:ObjectProperty ,
                owl:AsymmetricProperty ,
                owl:IrreflexiveProperty ;
        rdfs:domain :Edition ;
        rdfs:range :Organizer ;
        rdfs:label "is organized by"@en .

###  https://github.com/RiccardoOmiccioli/AMLO/organized
:organized rdf:type owl:ObjectProperty ;
        owl:inverseOf :isOrganizedBy ;
        rdfs:label "organized"@en .

###  https://github.com/RiccardoOmiccioli/AMLO/won
:won rdf:type owl:ObjectProperty ,
              owl:AsymmetricProperty ,
              owl:IrreflexiveProperty ;
        owl:inverseOf :isWonBy ;
        rdfs:domain mlo:Algorithms ;
        rdfs:range :Edition ;
        rdfs:label "won"@en .

###  https://github.com/RiccardoOmiccioli/AMLO/isWonBy
:isWonBy rdf:type owl:ObjectProperty ;
        rdfs:label "is won by"@en .

###  https://github.com/RiccardoOmiccioli/AMLO/isImplementationOf
:isImplementationOf rdf:type owl:ObjectProperty ,
                owl:FunctionalProperty ,
                owl:AsymmetricProperty ,
                owl:IrreflexiveProperty ;
        rdfs:domain foaf:Project ;
        rdfs:range mlo:Algorithms ;
        rdfs:label "is implementation of"@en .

###  https://github.com/RiccardoOmiccioli/AMLO/hasImplementation
:hasImplementation rdf:type owl:ObjectProperty ;
        owl:inverseOf :isImplementationOf ;
        rdfs:label "has implementation"@en .

###  https://github.com/RiccardoOmiccioli/AMLO/hasMLType
:hasMLType rdf:type owl:ObjectProperty ;
        owl:inverseOf :isMLTypeOf ;
        rdf:type owl:FunctionalProperty ,
                owl:AsymmetricProperty ,
                owl:IrreflexiveProperty ;
        rdfs:domain mlo:Algorithms ;
        rdfs:range :MachineLearningType ;
        rdfs:label "has ML type"@en .

###  https://github.com/RiccardoOmiccioli/AMLO/isMLTypeOf
:isMLTypeOf rdf:type owl:ObjectProperty ;
        rdfs:label "is ML type of"@en .

###  https://github.com/RiccardoOmiccioli/AMLO/hasReleaseYear
:hasReleaseYear rdf:type owl:ObjectProperty ;
        owl:inverseOf :isReleaseYearOf ;
        rdf:type owl:AsymmetricProperty ,
                owl:IrreflexiveProperty ;
        rdfs:domain mlo:Algorithms ;
        rdfs:range dbo:Year ;
        rdfs:label "has release year"@en .

###  https://github.com/RiccardoOmiccioli/AMLO/isReleaseYearOf
:isReleaseYearOf rdf:type owl:ObjectProperty ;
        rdfs:label "is release year of"@en .

###  https://github.com/RiccardoOmiccioli/AMLO/uses
:uses rdf:type owl:ObjectProperty ,
                owl:FunctionalProperty ,
                owl:AsymmetricProperty ,
                owl:IrreflexiveProperty ;
        rdfs:domain mlo:Algorithms ;
        rdfs:range :Tool ;
        rdfs:label "uses"@en .

###  https://github.com/RiccardoOmiccioli/AMLO/isUsedBy
:isUsedBy rdf:type owl:ObjectProperty ;
        owl:inverseOf :uses ;
        rdfs:label "is used by"@en .

###  https://github.com/RiccardoOmiccioli/AMLO/hasLayerType
:hasLayerType rdf:type owl:ObjectProperty ;
