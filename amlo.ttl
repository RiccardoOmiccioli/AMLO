###  http://www.a2rd.net.br/mlo#Institutions
mlo:Institutions rdf:type owl:Class ;
        rdfs:subClassOf foaf:Organization ;
        rdfs:isDefinedBy mlo: ;
        rdfs:label "Company"@en .


###  http://xmlns.com/foaf/0.1/Agent
foaf:Agent rdf:type owl:Class ;
        owl:disjointWith foaf:Project ,
                :Role ;
        owl:equivalentClass [ rdf:type owl:Class ;
                owl:unionOf ( foaf:Group
                        foaf:Person
                        foaf:Organization
                )
        ] ;
        rdfs:comment "An agent (eg. person, group, software or physical artifact)." ;
        rdfs:isDefinedBy foaf: ;
        rdfs:label "Agent" .


###  http://xmlns.com/foaf/0.1/Group
foaf:Group rdf:type owl:Class ;
        rdfs:subClassOf foaf:Agent ;
        rdfs:comment "A collection of individual agents." ;
        rdfs:isDefinedBy foaf: ;
        rdfs:label "Group" .


###  http://xmlns.com/foaf/0.1/Organization
foaf:Organization rdf:type owl:Class ;
        rdfs:subClassOf foaf:Agent ;
        owl:disjointWith foaf:Person ;
        owl:equivalentClass [ rdf:type owl:Class ;
                owl:unionOf ( mlo:Institutions
                        aiiso:Institute
                        aiiso:College
                )
        ] ;
        rdfs:comment "An organization." ;
        rdfs:isDefinedBy foaf: ;
        rdfs:label "Organization" .

### http://purl.org/vocab/aiiso/schema#Institute
aiiso:Institute rdf:type owl:Class ;
        rdfs:subClassOf foaf:Organization ;
        owl:disjointWith mlo:Institutions,
                aiiso:College ;
        rdfs:comment "An Institute is a group of people recognised by an organization as forming a cohesive group referred to by the organization as an institute." ;
        rdfs:isDefinedBy aiiso: ;
        rdfs:label "Institute" .

### http://purl.org/vocab/aiiso/schema#College
aiiso:College rdf:type owl:Class ;
        rdfs:subClassOf foaf:Organization ;
        owl:disjointWith mlo:Institutions,
                aiiso:Institute;
        rdfs:comment "A College is a group of people recognised by an organization as forming a cohesive group referred to by the organization as a college." ;
        rdfs:isDefinedBy aiiso: ;
        rdfs:label "College" .

###  http://xmlns.com/foaf/0.1/Person
foaf:Person rdf:type owl:Class ;
        rdfs:subClassOf foaf:Agent ;
        rdfs:comment "A person." ;
        rdfs:isDefinedBy foaf: ;
        rdfs:label "Person" .

###  https://github.com/RiccardoOmiccioli/AMLO/Role
:Role rdf:type owl:Class ;
        owl:disjointWith foaf:Project,
                foaf:Agent ;
        owl:equivalentClass [ rdf:type owl:Class ;
                owl:unionOf ( :Author
                        :Organizer
                )
        ] ;
        rdfs:label "Role" .

###  https://github.com/RiccardoOmiccioli/AMLO/Author
:Author rdf:type owl:Class ;
        rdfs:subClassOf :Role ;
        rdfs:comment "An author of a project." ;
        rdfs:label "Author" .

###  https://github.com/RiccardoOmiccioli/AMLO/Organizer
:Organizer rdf:type owl:Class ;
        rdfs:subClassOf :Role ;
        rdfs:comment "An organizer of an event." ;
        rdfs:label "Organizer" .

###  http://xmlns.com/foaf/0.1/Project
foaf:Project rdf:type owl:Class ;
        owl:disjointWith foaf:Agent,
                :Role ;
        rdfs:comment "A project (a collective endeavour of some kind)." ;
        rdfs:isDefinedBy foaf: ;
        rdfs:label "Project" .

