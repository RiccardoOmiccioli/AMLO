###  https://github.com/RiccardoOmiccioli/AMLO/involvment
:involvment rdf:type owl:ObjectProperty .

###  http://xmlns.com/foaf/0.1/fundedBy
foaf:fundedBy rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :involvment ;
        rdfs:domain foaf:Project ;
        rdfs:range foaf:Organization ;
        rdfs:comment "An organization funding a project." ;
        rdfs:isDefinedBy foaf: ;
        rdfs:label "is funded by" .

###  https://github.com/RiccardoOmiccioli/AMLO/funds
:funds rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf :involvment ;
       rdfs:label "funds" .

### https://github.com/RiccardoOmiccioli/AMLO/isDevelopedBy
:isDevelopedBy rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :involvment ;
        rdfs:domain foaf:Project ;
        rdfs:range foaf:Organization ;
        rdfs:comment "A project developed by an organization." ;
        rdfs:label "is developed by" .

###  https://github.com/RiccardoOmiccioli/AMLO/develops
:develops rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :involvment ;
          owl:inverseOf :isDevelopedBy ;
          rdfs:label "develops" .

###  https://github.com/RiccardoOmiccioli/AMLO/playsRole
:playsRole rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty .

###  https://github.com/RiccardoOmiccioli/AMLO/hasRole
:hasRole rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :playsRole ;
         owl:inverseOf :isRoleOf ;
         rdfs:domain foaf:Person ;
         rdfs:range :Role ;
         rdfs:comment "A person which has a role." ;
         rdfs:label "has role" .

###  https://github.com/RiccardoOmiccioli/AMLO/isRoleOf
:isRoleOf rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :playsRole ;
          rdfs:label "is role of" .

###  https://github.com/RiccardoOmiccioli/AMLO/membership
:membership rdf:type owl:ObjectProperty .

### https://github.com/RiccardoOmiccioli/AMLO/isMemberOf
:isMemberOf rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :membership ;
        rdfs:domain :Author ;
        rdfs:range foaf:Group ;
        rdfs:comment "An author which a member of a Group." ;
        rdfs:label "is member of" .

### https://github.com/RiccardoOmiccioli/AMLO/hasMember
:hasMember rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :membership ;
        owl:inverseOf :isMemberOf ;
        rdfs:label "has member" .

###  https://github.com/RiccardoOmiccioli/AMLO/employment
:employment rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty .

###  https://github.com/RiccardoOmiccioli/AMLO/isEmployedBy
:isEmployedBy rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :employment ;
              rdfs:domain :Organizer ;
              rdfs:range foaf:Organization ;
              rdfs:label "is employed by" .

###  https://github.com/RiccardoOmiccioli/AMLO/hasEmployee
:hasEmployee rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :employment ;
             owl:inverseOf :isEmployedBy ;
             rdfs:label "has employee" .

###  https://github.com/RiccardoOmiccioli/AMLO/isCoAuthor
:isCoAuthor rdf:type owl:ObjectProperty ;
        rdfs:domain :Author ;
        rdfs:range :Author ;
        owl:propertyChainAxiom ( :isMemberOf :hasMember ) .

###  https://github.com/RiccardoOmiccioli/AMLO/assignment
:assignment rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty .

###  https://github.com/RiccardoOmiccioli/AMLO/isAssignedTo
:isAssignedTo rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :assignment ;
              rdfs:domain foaf:Group ;
              rdfs:range foaf:Project ;
              rdfs:label "is assigned to" .

###  https://github.com/RiccardoOmiccioli/AMLO/hasAssigned
:hasAssigned rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :assignment ;
             owl:inverseOf :isAssignedTo ;
             rdfs:label "has assigned" .

