###  https://github.com/RiccardoOmiccioli/AMLO/assignment
:assignment rdf:type owl:ObjectProperty .

###  https://github.com/RiccardoOmiccioli/AMLO/isAssignedTo
:isAssignedTo rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :assignment ;
        rdfs:domain :Author ;
        rdfs:range foaf:Project ;
        rdfs:comment "An author working on a project." ;
        rdfs:label "is assigned to" .

### https://github.com/RiccardoOmiccioli/AMLO/hasAssigned
:hasAssigned rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :assignment ;
        owl:inverseOf :isAssignedTo ;
        rdfs:comment "An author which is assigned to the project" ;
        rdfs:label "has assigned" .

###  https://github.com/RiccardoOmiccioli/AMLO/involvment
:involvment rdf:type owl:ObjectProperty .

###  http://xmlns.com/foaf/0.1/fundedBy
foaf:fundedBy rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :involvment ;
        rdfs:domain foaf:Project ;
        rdfs:range foaf:Organization ;
        rdfs:comment "An organization funding a project." ;
        rdfs:isDefinedBy foaf: ;
        rdfs:label "is funded by" .

### https://github.com/RiccardoOmiccioli/AMLO/isDevelopedBy
:isDevelopedBy rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :involvment ;
        rdfs:domain foaf:Project ;
        rdfs:range foaf:Organization ;
        rdfs:comment "A project developed by an organization." ;
        rdfs:label "is developed by" .

###  https://github.com/RiccardoOmiccioli/AMLO/membership
:membership rdf:type owl:ObjectProperty .

###  http://xmlns.com/foaf/0.1/member
foaf:member rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :membership ;
        rdfs:domain foaf:Group ;
        rdfs:range foaf:Agent ;
        rdfs:comment "Indicates a member of a Group." ;
        rdfs:isDefinedBy foaf: ;
        rdfs:label "has member" .

### https://github.com/RiccardoOmiccioli/AMLO/isMemberOf
:isMemberOf rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :membership ;
        owl:inverseOf foaf:member ;
        rdfs:label "is member of" .

###  https://github.com/RiccardoOmiccioli/AMLO/hasColleague
:hasColleague rdf:type owl:ObjectProperty ;
        rdfs:domain foaf:Person ;
        rdfs:range foaf:Person ;
        owl:propertyChainAxiom ( :isMemberOf foaf:member ) .

###  https://github.com/RiccardoOmiccioli/AMLO/isCoAuthor
:isCoAuthor rdf:type owl:ObjectProperty ,
                owl:SymmetricProperty ;
        rdfs:domain :Author ;
        rdfs:range :Author .

